    <style name="Theme.mainActivity" parent="AppTheme">
        <!--在Android 4.4之前的版本上运行，直接跟随系统主题-->
        <!--底部黑色，状态栏黑色-->
    </style>

    <style name="Theme.artmofang" parent="AppTheme">
        <!--在Android 4.4之前的版本上运行，直接跟随系统主题-->
